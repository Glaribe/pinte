<div id="pins" class="transitions-enabled clearfix masonry">
  <% @pins.each do |pin| %>
    <div class="pin panel panel-default">
      <div class="panel-body">
        <img src="<%= pin.image_url %>" alt="<%= pin.title %>">
        <h2><%= pin.title %></h2>
        <p class="user">Publicado por <%= pin.user.email %></p>
        <div class="actions">
          	<span class="glyphicon glyphicon-heart"><%= link_to "", like_pin_path class: "btn btn-danger btn-block"
          	%></span>  
        </div>
      </div>
    </div>
  <% end %>
</div>